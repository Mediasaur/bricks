{% block content %}
<article{{ attributes }}>
  {% if content.body|render|striptags is not empty %}
    <section class="section-content">
      <div class="uk-container">
        {{ content.body }}
      </div>
    </section>
  {% endif %}
  <section class="section-custom-layouts">
    <div class="uk-container">
      {{ content.field_custom_layouts }}
    </div>
  </section>
</article>
{% endblock %}